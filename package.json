{
    "name": "testdeck-jasmine-seed",
    "version": "0.0.0",
    "private": true,
    "description": "A project with mocha-typescript enabled",
    "main": "index.js",
    "scripts": {
      "pretest": "tsc",
      "test": "nyc jasmine",
      "prepare": "tsc"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/testdeck/testdeck-jasmine-seed.git"
    },
    "author": "",
    "license": "Apache-2.0",
    "bugs": {
      "url": "https://github.com/testdeck/testdeck-jasmine-seed/issues"
    },
    "homepage": "https://github.com/testdeck/testdeck-jasmine-seed#readme",
    "dependencies": {
    },
    "devDependencies": {
      "@types/chai": "*",
      "@types/jasmine": "*",
      "@testdeck/jasmine": "*",
      "chai": "*",
      "jasmine": "*",
      "nyc": "*",
      "source-map-support": "*",
      "typescript": "*"
    },
    "nyc": {
      "check-coverage": true,
      "lines": 80,
      "statements": 80,
      "functions": 80,
      "branches": 80,
      "include": [
        "src/**/*.js"
      ],
      "exclude": [
        "test/**/*.js"
      ],
      "reporter": [
        "lcov",
        "text-summary"
      ],
      "all": true
    }
  }
  